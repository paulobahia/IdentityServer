datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        String   @id @default(uuid())
  name      String
  email     String   @unique
  password  String
  birthDate DateTime
  gender    String
  role      String
  createdAt DateTime @default(now()) @map("create_at")
  updateAt  DateTime @updatedAt @map("update_at")
}
